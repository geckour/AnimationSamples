<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="sample.satsukies.transitionsamples"
  >

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme"
    >
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".simple.SimpleFirstActivity" />
    <activity android:name=".simple.SimpleSecondActivity" />
    <activity android:name=".simple_shared.SimpleSharedFirstActivity" />
    <activity android:name=".simple_shared.SimpleSharedSecondActivity" />
    <activity
      android:name=".simple_shared_exclude.ExcludeSharedFirstActivity"
      android:theme="@style/AppTheme.Exclude"
      />
    <activity
      android:name=".simple_shared_exclude.ExcludeSharedSecondActivity"
      android:theme="@style/AppTheme.Exclude"
      />
  </application>

</manifest>